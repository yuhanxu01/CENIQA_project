{
  "backbone": "vit_small_patch16_224",
  "pretrained": true,
  "freeze_backbone": false,
  "feature_dim": 768,
  "hidden_dim": 512,
  "use_multi_scale": true,
  "use_frequency_features": true,
  "n_clusters": 8,
  "gmm_covariance_type": "diag",
  "gmm_init_method": "kmeans++",
  "gmm_update_freq": 10,
  "use_bic_selection": true,
  "min_clusters": 4,
  "max_clusters": 12,
  "regressor_type": "mlp",
  "use_monotonic": true,
  "dropout_rate": 0.2,
  "use_uncertainty": true,
  "batch_size": 32,
  "epochs": 100,
  "learning_rate": 0.0001,
  "weight_decay": 0.00001,
  "scheduler": "cosine",
  "warmup_epochs": 5,
  "lambda_contrast": 1.0,
  "lambda_consistency": 0.5,
  "lambda_ranking": 0.5,
  "lambda_cluster": 0.3,
  "lambda_quality": 1.0,
  "lambda_monotonic": 0.1,
  "image_size": 384,
  "num_crops": 10,
  "augmentation_strength": 0.5,
  "experiment_name": "ceniqa_baseline",
  "seed": 42,
  "use_wandb": false,
  "save_dir": "./experiments",
  "checkpoint_freq": 5
}
